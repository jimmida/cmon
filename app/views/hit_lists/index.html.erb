<h1>Listing hit_lists</h1>

<table>
  <thead>
    <tr>
      <th>Listing</th>
      <th>Filter</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @hit_lists.each do |hit_list| %>
      <tr>
        <td><%= hit_list.listing %></td>
        <td><%= hit_list.filter %></td>
        <td><%= link_to 'Show', hit_list %></td>
        <td><%= link_to 'Edit', edit_hit_list_path(hit_list) %></td>
        <td><%= link_to 'Destroy', hit_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

